                      <TableColumn fx:id="tcEmpresa" prefWidth="144.0" text="Empresa" />
                      <TableColumn fx:id="tcNombre" prefWidth="161.0" text="Nombre" />
                        <TableColumn fx:id="tcAp" prefWidth="164.0" text="Apellido Paterno" />
                        <TableColumn fx:id="tcAm" prefWidth="159.0" text="Apellido Materno" />
                        <TableColumn fx:id="tcCelular" prefWidth="181.0" text="Celular" />
                    </columns>
                  </TableView>
                  <Pagination fx:id="paginador" layoutX="6.0" layoutY="50.0" prefHeight="441.0" prefWidth="818.0" />
                  <Label layoutX="599.0" layoutY="454.0" text="Total:" />
                  <Label fx:id="lblRegistros" layoutX="683.0" layoutY="454.0" text="0 Registro(s)" />
                  <Label fx:id="lblMensaje" layoutX="490.0" layoutY="26.0" prefHeight="17.0" prefWidth="326.0" textFill="#e80909" />
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
   </children>
</AnchorPane>
