<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.RestaurarContacto">
   <children>
      <Label alignment="CENTER" prefHeight="22.0" prefWidth="850.0" text="Contactos Eliminados" textAlignment="JUSTIFY">
         <font>
            <Font name="Arial Black" size="20.0" />
         </font>
      </Label>
      <ToolBar layoutY="29.0" prefHeight="50.0" prefWidth="850.0">
        <items>
          <Button fx:id="btnRestaurar" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#click_restaurarC" prefHeight="40.0" prefWidth="120.0" text="Restaurar" />
        </items>
      </ToolBar>
      <TitledPane animated="false" layoutX="9.0" layoutY="81.0" prefHeight="518.0" prefWidth="832.0" text="Tabla">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="830.0">
               <children>
                  <TextField fx:id="txtBuscar" layoutX="14.0" layoutY="20.0" onKeyPressed="#buscarTexto" />
                  <Button fx:id="btnBuscar" alignment="CENTER_RIGHT" layoutX="179.0" layoutY="14.0" mnemonicParsing="false" onAction="#click_restaurarC" prefHeight="40.0" prefWidth="120.0" text="Buscar" />
                  <TableView fx:id="tvPC" layoutX="10.0" layoutY="61.0" onMouseClicked="#click_TableView" prefHeight="385.0" prefWidth="810.0">
